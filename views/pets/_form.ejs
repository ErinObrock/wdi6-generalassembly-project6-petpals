



	<div class="form-group">
		<label for="name"> Name </label>

		<input  class="form-control" type="text" id="name" name="name" value="<%= pet.name %>" />

	</div>
	<div class="form-group">
		<label for="age"> Age </label>
		<input class="form-control" type="number" id="age" name="age" value="<%= pet.age %>" />
	</div>
	<div class="form-group">
		<label for="type"> Type </label>
		<input class="form-control" type="text" id="type" name="type" value="<%= pet.type %>" />
	</div>
	<div class="form-group">
		<label for="breed"> Breed </label>
		<input class="form-control" type="text" id="breed" name="breed" value="<%= pet.breed %>" />
	</div>
	<div class="form-group">
		<label for="gender"> Gender </label>
		<input class="form-control" type="text" id="gender" name="gender" value="<%= pet.gender %>" />
	</div>
	<div class="form-group">
		<label for="bio"> Bio </label>
		<textarea class="form-control" rows="6" cols="20" type="text" name="bio" id="bio" placeholder="Tell us a little about yourself!" name="text"></textarea>
	</div>
	<div class="form-group">
		<label for="profilePicture"> Profile Picture </label>
		<input type="hidden" id="profilePicture" name="profilePicture" value=" https://s3-us-west-2.amazonaws.com/petpals/pet+pals-01.png " />
	</div>



