<% posts.forEach(function(post) { %>

<dl>
<dt><a href="/pets/<%= post.pet.id %>"> Name </a></dt>
     <dd><%= post.pet.name %></dd>
     </dt>
     <dd><%= post.pet.breed %></dd>
     </dt>
      <dd><%= post.pet.type %></dd>
      </dt>
    <dt> Title </dt><dd><%= post.title %></dd>
  <dt> Post </dt><dd><%= post.text %></dd>
  <dt> Pic</dt><dd><%= post.postPicture %></dd>
</dl>

<% }) %>
